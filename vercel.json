{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "SUPABASE_URL": "{{SUPABASE_URL}}",
        "SUPABASE_KEY": "{{SUPABASE_KEY}}"
    }
}