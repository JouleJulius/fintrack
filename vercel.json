{
    "version": 2,
    "framework": null,
    "functions": {
        "app.py": {
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "SUPABASE_URL": "{{SUPABASE_URL}}",
        "SUPABASE_KEY": "{{SUPABASE_KEY}}"
    }
}